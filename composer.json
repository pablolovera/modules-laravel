{
  "name": "pablolovera/modules-laravel",
  "type": "library",
  "description": "Modules for applications based on laravel 5.*",
  "keywords": ["modulos","modules", "laravel", "laravel 5 modules"],
  "homepage": "https://github.com/pablolovera/modules-laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Pablo Lovera",
      "email": "pablolovera180@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "nesbot/carbon": "~1.18",
    "illuminate/console": "~5.1 || ~5.2",
    "illuminate/support": "~5.1 || ~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "fabpot/php-cs-fixer": "~1.0",
    "orchestra/testbench": "~3.0",
    "fzaninotto/faker": "~1.5"
  },
  "autoload": {
    "psr-4": {
      "PabloLovera\\ModulesLaravel\\": "src/ModulesLaravel/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability" : "stable"
}